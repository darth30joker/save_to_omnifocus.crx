{
  "manifest_version": 2,

  "name": "Save to OmniFocus",
  "description": "This extension saves links to Omni Focus",
  "version": "1.0",

  "background": {
    "scripts": ["app.js"]
  },
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  }
}
